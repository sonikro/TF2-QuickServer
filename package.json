{
  "name": "tf2-quickserver",
  "version": "0.0.1",
  "description": "TF2 Quick Server Manager",
  "main": "index.js",
  "scripts": {
    "start": "tsx ./src/index.ts",
    "test": "vitest",
    "dev": "nodemon",
    "oracle:deploy": "terraform -chdir=./oracle-terraform init && terraform -chdir=./oracle-terraform apply -auto-approve && terraform -chdir=./oracle-terraform output -json > ./config/local.json",
    "download:maps": "tsx ./scripts/downloadMaps.ts",
    "migration:run": "knex --knexfile knexfile.ts migrate:latest",
    "migration:create": "knex --knexfile knexfile.ts migrate:make",
    "build:standard-competitive": "docker build -t sonikro/tf2-standard-competitive:latest -f ./variants/standard-competitive/Dockerfile ./variants/standard-competitive",
    "build:fat:standard-competitive": "docker build -t sonikro/fat-tf2-standard-competitive:latest -f ./variants/fat-standard-competitive/Dockerfile .",
    "push:fat:standard-competitive": "docker push sonikro/fat-tf2-standard-competitive:latest",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sonikro/TF2-QuickServer.git"
  },
  "keywords": [
    "tf2",
    "server",
    "bot",
    "discord",
    "manager",
    "oracle",
    "oci"
  ],
  "author": "sonikro",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sonikro/TF2-QuickServer/issues"
  },
  "homepage": "https://github.com/sonikro/TF2-QuickServer#readme",
  "devDependencies": {
    "@types/chance": "^1.1.6",
    "@types/config": "^3.3.5",
    "@types/express": "^5.0.1",
    "@types/node-schedule": "^2.1.7",
    "@types/supertest": "^6.0.3",
    "@types/websocket": "^1.0.10",
    "@vitest/coverage-v8": "^3.0.9",
    "msw": "^2.7.3",
    "node-bzip2": "^1.0.2",
    "nodemon": "^3.1.9",
    "supertest": "^7.1.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "vitest": "^3.0.9",
    "vitest-mock-extended": "^3.0.1",
    "vitest-when": "^0.6.0"
  },
  "dependencies": {
    "@adyen/api-library": "^26.1.0",
    "chance": "^1.1.12",
    "config": "^3.3.12",
    "discord.js": "^14.18.0",
    "dotenv": "^16.4.7",
    "express": "^5.1.0",
    "knex": "^3.1.0",
    "node-schedule": "^2.1.1",
    "oci-sdk": "^2.107.0",
    "p-limit": "^6.2.0",
    "rcon-client": "^4.2.5",
    "sqlite3": "^5.1.7",
    "ts-node": "^10.9.2",
    "uuid": "^11.1.0"
  }
}
